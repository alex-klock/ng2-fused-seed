{
  "compilerOptions": {
    
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es2015", "dom" ],
    "noEmit": true,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "../out",
    "pretty": true
  },

  "exclude": [
  ],

  "compileOnSave": false
}